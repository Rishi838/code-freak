#include <bits/stdc++.h>
#define m  int vec[10];  for(int i=0;i<10;i++)  cin>>vec[i]; int k;cin>>k;
using namespace std;
void func(){}
int main()
{
  m
   for(int i=9;i>=0;i--)
   {
       if(vec[i]>k || (k==0 && vec[i]!=0))
       {
       cout<<i+1;
       break;
       }
       
       k-=vec[i];
   }
	return 0;
}
