#include <bits/stdc++.h>
using namespace std;
void heapsort(int x[],int n){
    
}
void makeheap(int x[],int n){
      int a[2];
    heapsort(a,n);
    makeheap(a,n);
}
int main()
{
   int t,a,b;
   cin>>t;
   while(t--)
   {
       cin>>a>>b;
       int arr[a];
       for(int i=0;i<a;i++)
       cin>>arr[i];
       sort(arr,arr+a);
  
       int i=0,c=0,d=0;
       while(i<a && c+arr[i]<=b )
       {
           c+=arr[i];
           i++;
           d++;
       }
       cout<<d<<endl;
   }
	return 0;
}
