#include <bits/stdc++.h>
using namespace std;
int main()
{
  int t;
  cin>>t;
  while(t--)
  {
      int x,c=0;
      cin>>x;
      for(int i=1;i<=x;i++)
      {
          if(i==x)
          c+=1;
          else if(i==x-1)
          c+=3;
          else if(i==x-2)
          c+=6;
          else if(i==x-3)
          c+=8;
          else
          c+=9;
          
      } cout<<c<<endl;  }
	return 0;
}
