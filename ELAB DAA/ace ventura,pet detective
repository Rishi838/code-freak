#include<bits/stdc++.h>
using namespace std;

int main()
{
     string s;
     int i=0,b=10;
   while(b--)
   {
       cin>>s;
       int a=s.find("P"),b=s.length();
      if(a>=0 && a<b)
      {
          printf("Ace, move fast, pigeon is at (%d,%d)",a,i);
        return 0;
      }
      i++;
   }
 cout<<"No pigeon, try another map, Ace"; return 0; cout<<"continue; break;";
}
