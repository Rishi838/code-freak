//array as a arguement
#include<iostream>
using namespace std;

void print_array(int *arr,int n)
{
 for (int i = 0; i < n; i++)
 {
     cout<<*(arr+i)<<endl;
 }
  
}
void print_array1(int arr[],int n)
{
 for (int i = 0; i < n; i++)
 {
     cout<<arr[i]<<endl;
 }
     
}

int main()
{
    int arr[]={1,2,3,4,5};
    int n=5;
    print_array(arr,n);
    print_array1(arr,n);
    return 0;
}
