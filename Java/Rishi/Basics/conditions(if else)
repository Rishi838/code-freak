import java.util.Scanner;

class subscribe {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter you age: ");
        int a = sc.nextInt();
        if (a > 18)
            System.out.println("You can drive");
        else
            System.out.println("You cannot drive");
    }
}
