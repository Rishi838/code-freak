from tkinter import *
root=Tk()
root.geometry("644x344")
root.title("First GUI")
Label(root,text="Welcome to Rishi Travels",font="comicsansms 13 bold",pady=10).grid(row=0,column=6)
name=Label(root,text="Name")
phone=Label(root,text="Phone No.")
address=Label(root,text="Address")
gender=Label(root,text="Gender")
name.grid(row=2,column=2)
phone.grid(row=3,column=2)
address.grid(row=4,column=2)
gender.grid(row=5,column=2)

name_val=StringVar()
phone_val=StringVar()
address_val=StringVar()
gender_val=StringVar()
prserve=IntVar()

name_entry=Entry(root,textvariable=name_val)
phone_entry=Entry(root,textvariable=phone_val)
address_entry=Entry(root,textvariable=address_val)
gender_entry=Entry(root,textvariable=gender_val)
preserve=Checkbutton(root,text="You  agree to terms and conditions",variable=prserve)

name_entry.grid(row=2,column=3)
phone_entry.grid(row=3,column=3)
address_entry.grid(row=4,column=3)
gender_entry.grid(row=5,column=3)
preserve.grid(row=6,column=3)

def getvals():
    print("Name: ",name_val.get())
    print("Phone: ",phone_val.get())
    print("Address: ",address_val.get())
    print("Gender: ",gender_val.get())
Button(text="Submit to harry travels",command=getvals).grid(column=3)
root.mainloop()
