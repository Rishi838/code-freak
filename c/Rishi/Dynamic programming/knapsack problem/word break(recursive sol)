#include <bits/stdc++.h>
using namespace std;

vector<string> vec{"mobile", "samsung", "sam", "sung",
                   "man", "mango", "icecream", "and",
                   "go", "i", "like", "ice", "cream"};

bool contains(string word)
{
   for (int i = 0; i < vec.size(); i++)
   {
      if (vec[i]==word)
         return true;
   }
   return false;
}

bool dictionary_contains(string s)
{
   if (s.size() == 0)
      return true;

   for (int i = 1; i <= s.size(); i++)
   {
      if (contains(s.substr(0, i)) && dictionary_contains(s.substr(i)));
         return true;
   }
   return false;
}

int main()
{
   cout<<dictionary_contains("ilikesamsungmobileansicecream");
   return 0;
}
