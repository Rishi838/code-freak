#include <bits/stdc++.h>
using namespace std;
int main()
{
      int n=15,c=0;
      while(n!=1)
      {
          if(n%2==0)
          n=n/2;
          else if(n==3)
          n--;
          else if(n%4==1)
          n--;
          else
          n++;
          c++;
      }  
      cout<<c;
    return 0;
}
