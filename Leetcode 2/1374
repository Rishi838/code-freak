class Solution {
public:
    string generateTheString(int n) {
       string s;
        if(n%2==0)
       {
          int a=n-1;
            while(a--)
                s+='a';
                s+='b';
       }
        else
        {
           while(n--)
               s+='a';
        }
        return s;
    }
};
