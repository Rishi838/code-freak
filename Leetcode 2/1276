class Solution {
public:
    vector<int> numOfBurgers(int t, int c) {
        vector<int> v;
        for(int i=0;i<=c;i++)
        {
            if((2*i) + (4*(c-i)) == t )
            {
                 v.push_back(c-i);
                v.push_back(i);
                return v;
            }
        }
        return v;
    }
};
