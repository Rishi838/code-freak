class Solution {
public:
    vector<vector<int>> construct2DArray(vector<int>& vec, int m, int n) {
        vector<vector<int>> v(m,vector<int>(n));
        if(m*n!=vec.size())
        {
        vector<vector<int>> v1;
        return v1;
        }
        int a=0;
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<n;j++)
            {
                v[i][j]=vec[a];
                a++;
            }
        }
        return v;
        
    }
};
