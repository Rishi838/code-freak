class Solution {
public:
    int maxSum(vector<vector<int>>& a) {
        int sum=0;
        for(int i=0;i<=a.size()-3;i++)
        {
            for(int j=0;j<=a[0].size()-3;j++)
            {
                int s = a[i][j] + a[i][j+1] + a[i][j+2] + a[i+1][j+1] + a[i+2][j] + a[i+2][j+1] + a[i+2][j+2];
                sum=max(sum,s);
                cout<<s<<endl;
            }
        }
        return sum;
    }
};
