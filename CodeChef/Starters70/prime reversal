#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	scanf("%d",&t);
	while(t--)
	{
	    int n;
	    scanf("%d",&n);
	    string a, b;
	    cin>>a>>b;
	    if(count(a.begin(), a.end(), '0')==count(b.begin(), b.end(), '0'))
	    printf("YES\n");
	    else
	    printf("NO\n");
	}
	return 0;
}
