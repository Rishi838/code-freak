// Follwing is the object literal
let car ={
    name : 'Mercedes',
    topspeed : 200,
    run : function()
    {
     console.log("car is running");
    }
}
//Follwing is the constructor
function general_car(givenname,givenspeed)
{
    this.name=givenname;
    this.topspeed=givenspeed;
    this.run = function()
    {
        console.log(`${this.name} is running`);
    }
}
//Creating object using new keyword
nisan = new general_car("nissan",180);
