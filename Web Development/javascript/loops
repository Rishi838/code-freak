// for loop
for(let i=0;i<3;i++)
{
console.log(i);
}
//While loop
let i=0;
while(i<5)
{
console.log(i);
i++;
}
//Do while loop
do{
console.log(i);
i++;
}while(i<10);


// For arrays

arr=[1,2,3,4];
arr.forEach(function(element,index,array) {
console.log(element,index,array);
});


//For objects

let obj ={
     name :'rishi''
     age :20
 }
 for(let key in obj)
 {
console.log(`Value at ${key} is obj[${key}]`);
}
