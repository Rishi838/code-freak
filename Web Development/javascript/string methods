str='Hello ';
str = str.concat('World');
console.log(str);
console.log(str.length);   // Function nhi hai ye
str=str.toLowerCase();    // Ye function changes nhi kr rha ye bus return kr rha hai
console.log(str);
console.log(str.toUpperCase());
console.log('Element at index 0 is: ' + str[0]);
console.log(str.indexOf('ll'));// Returns -1 if string is not present  also it returns first occurence of that string
//console.log(str.lastIndexOf('ll')     This returns last index of enterd string
console.log(str.charAt(0));
console.log(str.endsWith('orld'));
console.log(str.includes('Hello'));
console.log(str.substring(1,3));    // 1 se start hokr 3-1 tk
console.log(str.slice(-4,-2));   //Returns last 4 character ,it is identical to substring but for -4,substring returns full string
//Last Index is-1,same as python list slicing
console.log(str.split('o'));    //Splits at index with charcter 'o' without including it
console.log(str.replace('world','rishi'));    // it replaces first occurennce of world with rishi
