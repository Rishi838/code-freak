#include <iostream>
using namespace std;
int main()
{
      int N,i;
      cin>>N;
      for(i=1; i<=N; i++)
      {
          int a=1;
          if(i%2==0)
          a=2;
          int c=0;
          while(c++<i)
          {
              cout<<a<<" ";
              a=a+2;
          }
          cout<<"\n";
      }
	return 0;
}
