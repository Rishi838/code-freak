#include <iostream>
using namespace std;
int main()
{
 int cnt;
 cin>>cnt;
 if(cnt==0)
 {
     cout<<"0 0";
 }
 else
 {
 int arr[cnt],a=0,num=cnt,sum=0;
 for(int i=0;i<cnt;i++)
 cin>>arr[i];
  while(num)
  {
      if(arr[num-1]>=0)
      {
          sum+=arr[num-1];
          a++;
          
      }
      num--;
  }
  cout<<sum<<" "<<a;
 }
	return 0;
}
