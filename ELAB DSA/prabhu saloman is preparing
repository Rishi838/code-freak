#include <bits/stdc++.h>
using namespace std;
int main()
{
   int T,n,y;
   cin>>T;
 for(int t=0;t<T;t++)
   {
       cin>>n>>y;
       int arr[n];
       for(int i=0;i<n;i++)
       cin>>arr[i];
     for(int i=n-1;i>=0;i--)
       {
           int a=y/arr[i];
           y=a*arr[i];
       }
       cout<<y<<endl;
   }
	return 0;
}
