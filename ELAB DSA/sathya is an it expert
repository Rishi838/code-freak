#include <bits/stdc++.h>
using namespace std;
int main()
{
int t;
cin>>t;
while(t>0)
{
    int n,a,b,sum=0;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    cin>>arr[i];
    cin>>a>>b;
    sort(arr,arr + n);
    for(int i=0;i<n-1;i++)
    sum=0;
    for(int i=a;i<b-1;i++)
    sum+=arr[i];
    cout<<sum<<"\n";
    t--;
}
	return 0;
}
