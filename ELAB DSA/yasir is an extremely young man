#include<bits/stdc++.h>
using namespace std;
int main()
{
  int n,q,a;
  cin>>n>>q;
  set<int> s;
  for(int i=0;i<n;i++){
  cin>>a;
  s.insert(a);
  }
  int first=1,last=q;
  while(first<=last)
  {
      cin>>a;
      if(s.find(a)!=s.end())
      cout<<"YES\n";
      else
      cout<<"NO\n";
     first++;
  }
	return 0;
}
