#include<iostream>
using namespace std;
int main()
{
    int row,i=1,a,j;
    cin>>row;
    row--;
    for(j=row;j>=0;j--)
    {
        cin>>a;
        cout<<"Process #"<<i<<":\n";
        i++;
        int c=1;
        while(c<=a)
        {
          cout<<c<<" "<<c<<"\n";
          c++;
        }
    }
    return 0;
}
