ls=[1,2,3,4,5,6,7,8]
for i in ls:
    print(i,end=" ")
else:
    print("\nLoop Ended Normally")

for i in ls:
    print(i,end=" ")
    if i==3:
        break
else:
    print("\nLoop Ended Normally")

#use is to serach a element in anything
for i in ls:
    if i==3:
        print("\n3 found")
        break
else:
    print("3 not found")
